-- Scripts:

local function GKCF_fake_script() -- FPSText.LocalScript 
	local script = Instance.new('LocalScript', FPSText)

	while wait() do
		local fps = game.Workspace:GetRealPhysicsFPS()
		script.Parent.Text = "FPS " ..fps
		wait()
	end
end
coroutine.wrap(GKCF_fake_script)()
local function KRYFKE_fake_script() -- UsernameText.UsernameScript 
	local script = Instance.new('LocalScript', UsernameText)

	local script = Instance.new('LocalScript', UsernameScript)
	
	local user = game.Players.LocalPlayer.Name
	local txt = script.Parent
	
	txt.Text = "Hello," ..user
end
coroutine.wrap(KRYFKE_fake_script)()
local function YMRXP_fake_script() -- CashText.LocalScript 
	local script = Instance.new('LocalScript', CashText)

	local text = script.Parent.text
	
	while true do
		wait(0.1)
		text = game.Players.LocalPlayer.DataFolder.Currency.Value
	end
end
coroutine.wrap(YMRXP_fake_script)()



