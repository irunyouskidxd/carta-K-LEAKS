coroutine.wrap(function()
	local Tries = 1
	repeat wait(1)Tries = 1 pcall(setfpscap, getgenv().FPS) -- finding the local fps 
	pcall(set_fps_cap, getgenv().FPS) until Tries > 50
end)()
