local function XRLVJ_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	while true do 
		wait(0.5)
		script.Parent.Text = game.Players.LocalPlayer.DataFolder.Currency.Value
	end
	
end
coroutine.wrap(XRLVJ_fake_script)()
local function WFAMGVI_fake_script() -- MainTab.user thumbnail 
	local script = Instance.new('LocalScript', MainTab)

	local Frame = script.Parent
	
	local player = game.Players.LocalPlayer
	
	local userId = player.UserId
	local thumtype = Enum.ThumbnailType.AvatarBust
	local thumsize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(userId, thumtype, thumsize)
	Frame.PlayerImage.Image = content
	Frame.PlayerName.Text = player.Name
end
coroutine.wrap(WFAMGVI_fake_script)()
local function LUFRDRG_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.MainTab.Visible = true
		script.Parent.Parent.LocationTab.Visible = false
		script.Parent.Parent.creditsTab.Visible = false
		script.Parent.Parent.droppingTab.Visible = false
		wait(1)
	end)
end
coroutine.wrap(LUFRDRG_fake_script)()
